{
	"name": "dotjs universal",
	"manifest_version": 2,
	"minimum_chrome_version": "23",
	"version": "1.5.0",
	"description": "~/.js",
	"icons": {
		"48": "images/icon48.png",
		"128": "images/icon128.png"
	},
	"options_page": "options.html",
	"content_scripts": [{
		"all_frames": true,
		"run_at": "document_end",
		"matches": ["http://*/*", "https://*/*"],
		"js": ["jquery.js", "dotjs.js"]
	}],
	"web_accessible_resources": ["scripts/*.js", "styles/*.css"]
}
